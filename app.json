{
  "expo": {
    "name": "typescript-template",
    "slug": "typescript-template",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "react-native-auth0",
        {
          "domain": "dev-66atc3o2x4zs0mz8.us.auth0.com",
          "customScheme": "mynewschema"
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.auth0.robota"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
